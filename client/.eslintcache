ek/coding/book-notes/client/src/hooks/editBook.js",["108"],"import { useEffect, useState } from 'react';\n\nconst useEditBook = ({ activeBook: book, editType, edits, chapterId }) => {\n  const [data, setData] = useState();\n  let editedBook = { ...book };\n  let chapterIndex = editedBook.chapters?.findIndex(\n    (chapter) => chapter.id === chapterId\n  );\n\n  const handleBook = () => {};\n\n  const handleChapter = () => {\n    let chapters = editedBook.chapters;\n    chapterIndex === -1\n      ? chapters.push(edits)\n      : (chapters[chapterIndex] = { ...chapters[chapterIndex], ...edits });\n    let sorted = chapters.sort((a, b) => {\n      return a.chapterNumber - b.chapterNumber;\n    });\n    editedBook.chapters = sorted;\n    setData(editedBook);\n  };\n\n  const handleNote = () => {\n    let noteIndex = editedBook.chapters[chapterIndex].notes.findIndex(\n      (note) => note.id === edits.id\n    );\n    let notes = editedBook.chapters[chapterIndex].notes;\n    noteIndex === -1\n      ? notes.push(edits)\n      : (notes[noteIndex] = { ...notes[noteIndex], ...edits });\n    let sorted = notes.sort((a, b) => {\n      return a.pageNumber - b.pageNumber;\n    });\n    editedBook.notes = sorted;\n    setData(editedBook);\n  };\n\n  useEffect(() => {\n    switch (editType) {\n      case 'book':\n        handleBook();\n        break;\n      case 'chapter':\n        handleChapter();\n        break;\n      case 'note':\n        handleNote();\n        break;\n      default:\n    }\n  }, [edits, editType]);\n\n  return { data };\n};\n\nexport default useEditBook;\n","/home/derek/coding/book-notes/client/src/components/Sidebar/Blurb.js",[],"/home/derek/coding/book-notes/client/src/components/Buttons/elements/SVGButton.js",[],"/home/derek/coding/book-notes/client/src/components/Buttons/svg/Plus.js",[],"/home/derek/coding/book-notes/client/src/components/Buttons/svg/Pencil.js",[],"/home/derek/coding/book-notes/client/src/components/Buttons/svg/ChevronDown.js",[],"/home/der